{
  "name": "book-hub",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "studio": "npx prisma studio",
    "migrate_db": "npx prisma migrate dev --preview-feature",
    "reset_db": "npx prisma migrate reset --force --preview-feature",
    "nexus_gen": "pnpm -s run generate:prisma && pnpm -s run generate:nexus",
    "gql_gen": "graphql-codegen --config codegen.yml",
    "gql_gen:watch": "graphql-codegen --config codegen.yml --watch",
    "storybook": "start-storybook -s ./public -p 6006",
    "test": "jest",
    "ts": "tsc --noEmit",
    "prettier": "npx prettier --write .",
    "analyze": "ANALYZE=true next dev",
    "build": "next build",
    "start": "next start",
    "build-storybook": "build-storybook",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "prepare": "husky install",
    "lint": "next lint --fix"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@radix-ui/react-avatar": "0.1.1",
    "@radix-ui/react-dropdown-menu": "0.1.1",
    "@radix-ui/react-id": "0.1.1",
    "@radix-ui/react-progress": "0.1.1",
    "@radix-ui/react-switch": "0.1.1",
    "@splitbee/web": "0.3.0",
    "@tailwindcss/line-clamp": "0.3.0",
    "@tanem/react-nprogress": "3.0.81",
    "apollo-server-micro": "2.19.2",
    "classnames": "2.3.1",
    "date-fns": "2.25.0",
    "express": "4.17.1",
    "graphql": "15.5.1",
    "graphql-iso-date": "3.6.1",
    "iso-639-1": "2.1.10",
    "next": "12.0.1",
    "next-auth": "4.0.5",
    "next-pwa": "5.4.0",
    "nexus": "1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.19.5",
    "react-hot-toast": "2.1.1",
    "react-markdown": "7.1.0",
    "react-spring": "9.3.0",
    "react-use-gesture": "9.1.3",
    "slugify": "1.6.1",
    "ts-node": "10.4.0",
    "zustand": "3.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/named-operations-object": "2.2.0",
    "@graphql-codegen/typescript": "2.3.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.0",
    "@next/bundle-analyzer": "12.0.2",
    "@prisma/client": "3.7.0",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.3.12",
    "@types/classnames": "2.3.1",
    "@types/graphql-iso-date": "3.4.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/tailwindcss": "2.2.4",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "autoprefixer": "10.4.0",
    "babel-loader": "8.2.3",
    "bundlemon": "1.1.1",
    "chromatic": "6.0.5",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "msw": "0.35.0",
    "msw-storybook-addon": "1.4.1",
    "postcss": "8.3.11",
    "prettier": "2.4.1",
    "prisma": "3.6.0",
    "sass": "1.43.4",
    "storybook-css-modules-preset": "1.1.1",
    "tailwindcss": "3.0.1",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.ts": "jest --passWithNoTests --findRelatedTests --bail",
    "*.{ts, tsx}": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "babel": {
    "presets": [
      "next/babel",
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
